{
  "Application": {
    "Name": "WebClient",
    "Version": "0.1.0"
  },
  "AuthenticationSettings": {
    "AuthorityUrl": "http://localhost:5001/",
    "UseApi": true,
    "UseWeb": true,
    "ApiSettings": {
      "Audience": "api",
      "ClientId": "api",
      "ClientSecret": "024f8e6c-d72f-4dad-975f-3bfbfc922427",
      "RequireHttps": false
    },
    "WebSettings": {
      "Resource": "http://localhost:5000/",
      "ClientId": "mvc",
      "ClientSecret": "901564A5-E7FE-42CB-B10D-61EF6A8F3654"
    },
    "GoogleCalendar": {
      "Username": "username",
      "ClientId": "clientid",
      "ClientSecret" :  "clientsecret"
    } 
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "LiterateConsole"
            },
            {
              "Name": "RollingFile",
              "Args": {
                "pathFormat": "Logs/log-{Date}.txt",
                "shared": true
              }
            }
          ]
        }
      }
    ]
  }
}